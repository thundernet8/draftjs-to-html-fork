    "description": "A library for draftjs to html conversion.",
    "main": "lib/draftjs-to-html.js",
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-eslint": "^8.0.1",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "chai": "^4.1.0",
        "draft-js": "^0.10.1",
        "enzyme": "^3.1.0",
        "eslint": "^4.3.0",
        "eslint-config-airbnb": "^16.0.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-mocha": "^4.11.0",
        "eslint-plugin-react": "^7.1.0",
        "flow-bin": "^0.56.0",
        "jsdom": "^11.1.0",
        "mocha": "^4.0.1",
        "react": "^16.0.0",
        "react-addons-test-utils": "^15.6.0",
        "react-dom": "^16.0.0",
        "rimraf": "^2.6.1",
        "sinon": "^4.0.1",
        "webpack": "^3.3.0",
        "webpack-dev-middleware": "^1.11.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/thundernet8/draftjs-to-html-fork.git"
    },
    "scripts": {
        "clean": "rimraf lib",
        "build:webpack":
            "NODE_ENV=production webpack --config config/webpack.prod.config.js",
        "build": "npm run clean && npm run build:webpack",
        "test":
            "mocha --compilers js:config/test-compiler.js config/test-setup.js js/**/*Test.js",
        "lint": "eslint js",
        "flow": "flow; test $? -eq 0 -o $? -eq 2",
        "check": "npm run lint && npm run flow"
    },
